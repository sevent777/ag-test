{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*",
        "src/types/*"
      ]
    },
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es7"
    ],
    "module": "commonjs",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "jsx": "react"
  },
}